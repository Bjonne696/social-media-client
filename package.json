    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "live-server": "^1.2.2",
    "prettier": "3.1.0",
    "sass": "^1.54.8"
